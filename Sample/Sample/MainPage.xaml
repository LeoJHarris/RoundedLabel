<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:Sample"
             x:Class="Sample.MainPage">
    <StackLayout Spacing="0" Orientation="Vertical" VerticalOptions="Center" HorizontalOptions="FillAndExpand">
        <ContentView HorizontalOptions="Center" VerticalOptions="Center" x:Name="OnlyRoundedLabelView"/>
        <ContentView HorizontalOptions="Center" VerticalOptions="Center" x:Name="RoundedLabelViewInsideCustomContentView"/>
    </StackLayout>
</ContentPage>
